    "Muestra Swap": {
      "Type": "Parallel",
      "Next": "Publicar",
      "Branches": [
        {
          "StartAt": "Configurar Publicacion",
          "States": {
            "Configurar Publicacion": {
              "Type": "Pass",
              "Parameters": {
                "swap_curve.$": "$.swap_curve",
                "intraday.$": "$.intraday",
                "valuation_date.$": "$.valuation_date",
                "output_file_name.$": "$.output_file_name",
