                        "today_data.$": "$[1].today_data"
                      }
                    },
                    "Validador Muestra": {
                      "Type": "Task",
                      "Resource": "arn:aws:states:::lambda:invoke",
                      "OutputPath": "$.Payload",
                      "Parameters": {
