        }
      ]
    },
    "Config Crear event muestra": {
      "Type": "Pass",
      "Parameters": {
        "swap_curve.$": "$[0].swap_curve",
        "intraday.$": "$[0].intraday",
        "valuation_date.$": "$[0].valuation_date",
        "precia_api.$": "$[0].precia_api",
        "output_file_name.$": "$[0].output_file_name",
        "error_email.$": "$[0].error_email",
